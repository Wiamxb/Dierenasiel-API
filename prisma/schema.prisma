generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Dier {
  id           Int      @id @default(autoincrement())
  naam         String
  soort        String
  leeftijd     Int
  beschrijving String?
  aangemaakt   DateTime @default(now())
}

model Faq {
  id         Int      @id @default(autoincrement())
  vraag      String
  antwoord   String
  aangemaakt DateTime @default(now())
}
